    "react": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@styled-icons/material": "^10.47.0",
    "@styled-icons/material-outlined": "^10.47.0",
